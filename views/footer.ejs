 <script>
   document.addEventListener('DOMContentLoaded', function () {
     // Variable dari server-side
     const isFree = <%= subscription.plan_name === "Gratis" ? "true" : "false" %>;

     if (isFree) {
       // Ambil semua link dengan class 'restricted'
       const restrictedLinks = document.querySelectorAll('.nav-link.restricted');

       restrictedLinks.forEach(function (link) {
         link.addEventListener('click', function (e) {
           e.preventDefault(); // Mencegah navigasi
           showUpgradePopup();
         });
       });
     }

     // Popup elements
     const upgradePopup = document.getElementById('upgradePopup');
     const closeBtn = document.querySelector('.upgrade-popup-content .close-btn');

     // Function to show popup
     function showUpgradePopup() {
       upgradePopup.style.display = 'flex';
     }

     // Function to hide popup
     function hideUpgradePopup() {
       upgradePopup.style.display = 'none';
     }

     // Event listeners for closing the popup
     closeBtn.addEventListener('click', hideUpgradePopup);
     window.addEventListener('click', function (event) {
       if (event.target === upgradePopup) {
         hideUpgradePopup();
       }
     });
   });
 </script>
